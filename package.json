{
  "name": "demo",
  "version": "0.0.1",
  "description": "订单",
  "main": "./dist/index.js",
  "repository": "git@git.17usoft.com:fsd/demo.git",
  "scripts": {
    "test": "one-jest",
    "testall": "./node_modules/.bin/jest --coverage",
    "build": "rm -rf ./dist && tsc",
    "debug": "cross-env NODE_ENV=dev tsc && node --inspect-brk ./dist/index.js",
    "kill": "pm2 kill",
    "run": "cross-env NODE_ENV=dev tsc && node ./dist/index.js ",
    "start": "cross-env NODE_ENV=prod tsc && pm2 restart ./processes.json --env prod",
    "reload": "cross-env NODE_ENV=prod pm2 reload ./processes.json --env prod",
    "stop": "pm2 stop ./processes.json",
    "delete": "pm2 delete ./processes.json",
    "typings": "./node_modules/.bin/ts-node ./node_modules/.bin/typings",
    "dev": " cross-env NODE_ENV=dev nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^24.0.11",
    "cross-env": "^5.2.0",
    "te-one": "^1.0.22-alpha.2",
    "te-one-jest": "^0.0.8",
    "te-one-typings": "^0.0.13"
  },
  "devDependencies": {
    "@types/node": "^10.0.0",
    "husky": "^1.1.3",
    "nodemon": "^1.18.6",
    "pm2": "^3.2.2",
    "ts-node": "^7.0.1",
    "typescript": "3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-push": "tsc"
    }
  }
}
